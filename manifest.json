{
  "manifest_version": 3,
  "name": "ChatGPTools",
  "version": "1.2.1",
  "permissions": [],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
	  {
      "matches": ["https://chat.openai.com/*"],
	  "run_at": "document_end",
      "js": ["content.js"]
    }
  ]
}
